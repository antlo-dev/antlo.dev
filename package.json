    "svelte": "3.46.4",
    "postcss": "8.4.12",
    "postcss-load-config": "3.1.1",
    "svelte-preprocess": "4.10.1",
    "autoprefixer": "10.4.4",
    "tailwindcss": "3.0.23"
  },
  "type": "module"
